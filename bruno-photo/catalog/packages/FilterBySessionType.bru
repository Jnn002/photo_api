meta {
  name: Filter Packages By Session Type
  type: http
  seq: 5
}

get {
  url: {{back}}/packages?session_type=Studio&limit=100
  body: none
  auth: inherit
}

params:query {
  session_type: Studio
  limit: 100
}

headers {
  Authorization: Bearer {{token}}
}

tests {
  test("should return 200", function() {
    expect(res.status).to.equal(200);
  });

  test("should return filtered packages", function() {
    const data = res.body;
    expect(Array.isArray(data)).to.be.true;
  });
}
