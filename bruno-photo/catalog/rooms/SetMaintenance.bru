meta {
  name: Set Room Maintenance
  type: http
  seq: 7
}

put {
  url: {{back}}/rooms/:room_id/maintenance
  body: none
  auth: inherit
}

params:path {
  room_id: 1
}

headers {
  Authorization: Bearer {{token}}
}

tests {
  test("should return 200", function() {
    expect(res.status).to.equal(200);
  });

  test("should return room in maintenance", function() {
    const data = res.body;
    expect(data).to.have.property('status');
    expect(data.status).to.equal('Maintenance');
  });
}
